{
  "name": "addachapter",
  "version": "1.0.0",
  "description": "Homebrewery as an electron app",
  "main": "main/main.js",
  "bin": {
    "webpack": "webpack",
    "electron": "electron",
    "electron-packager": "electron-packager"
  },
  "scripts": {
    "start": "webpack && cp LICENSE build/ && electron build/main/main.js",
    "webpack-dev": "webpack && cp LICENSE build/",
    "package-win": "electron-packager ./build add-a-chapter --overwrite --platform=win32 --arch=ia32 --prune=true --out=packaged --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='add-a-chapter'",
    "package-win-exe": "./installer-scripts/create-windows-installer.js",
    "package-linux": "electron-packager ./build add-a-chapter --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=packaged",
    "package-linux-debian": "time electron-installer-debian --src packaged/add-a-chapter-linux-x64/ --dest dist/installers/ --arch amd64 --config=debian.json",
    "pack": "electron-builder --projectDir ./build -lw"
  },
  "keywords": [
    "electron",
    "homebrewery",
    "dnd",
    "d&d"
  ],
  "author": "heath gehlhausen",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "eslint-utils": "^1.4.2",
    "marked": "^0.7.0",
    "monaco": "^1.201704190613.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-monaco-editor": "^0.30.1",
    "react-resize-detector": "^4.2.0",
    "reactstrap": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-property-literals": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "electron": "^5.0.9",
    "electron-builder": "^21.2.0",
    "electron-installer-debian": "^2.0.0",
    "electron-packager": "^14.0.4",
    "electron-winstaller": "^4.0.0",
    "eslint": "^6.1.0",
    "file-loader": "^4.2.0",
    "generate-json-webpack-plugin": "^0.3.1",
    "handlebars": "^4.3.0",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "monaco-editor": "^0.17.1",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "prop-types": "^15.7.2",
    "sass-loader": "^7.2.0",
    "serialize-javascript": "^3.1.0",
    "terser-webpack-plugin": "^1.4.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  }
}
